<tr class="<%= cycle('even', 'odd') %>">
  <td><%= ticket.uuid %></td>
  <td><%= ticket.current_status %></td>
  <td><%= ticket.current_support %></td>
  <td><%= h ticket.subject %></td>

  <%# if self.action_name == 'index' %>
      <td><%= h ticket.problem %></td>

      <td align='center'>
        <%= link_to image_tag('show-icon.png', alt: ''), ticket_path(ticket) %>
        <%= link_to image_tag('edit-icon.png', alt: ''), edit_ticket_path(ticket) %>

        <% if admin? %>
          <%= link_to image_tag('delete-trash.png', alt: ''), ticket_path(ticket), method: :delete, data: {confirm: 'Are you sure?'} %>
        <% end %>
      </td>
  <%# end %>
</tr>
