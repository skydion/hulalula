<%= form_for :support, url: authenticate_path do |f| %>
    <div style="text-align: right;">
      <div>
        <%= f.label :login, class: 'label' %>
        <%= f.text_field :login %>
      </div>

      <div>
        <%= f.label :password, class: 'label' %>
        <%= f.password_field :password %>
      </div>

      <%= f.submit('Login') %>
    </div>
<% end %>
