<% @roles = Role.all %>

<table>
  <tr>
    <th>Login</th>
    <th>Password</th>
    <th>First name</th>
    <th>Last name</th>
    <th>E-mail</th>
    <th>Role</th>
  </tr>

  <% @supports.each do |support| %>
    <%= render partial: 'supports/table_row', locals: {support: support} %>
  <% end %>
</table>

<% if session[:role_name] == 'admin' %>
    <%= link_to 'New Support', new_support_path %>
<% end %>
